    "codelyzer": "~1.0.0-beta.2",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "css-to-string-loader": "^0.1.2",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
