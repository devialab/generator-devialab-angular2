    "ts-helpers": "1.1.2",
    "ts-node": "^1.3.0",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.3",
    "typedoc": "^0.5.0",
    "typescript": "^2.1.0-dev.20161110",
    "underscore-deep-extend": "^1.1.5",
